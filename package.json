{
  "name": "receptionalx",
  "private": true,
  "scripts": {
    "dev": "pnpm --filter @receptionalx/web dev",
    "dev:web": "pnpm --filter @receptionalx/web dev",
    "dev:relay": "pnpm --filter @receptionalx/relay dev",
    "build": "pnpm -r build",
    "build:web": "pnpm --filter @receptionalx/web build",
    "build:relay": "pnpm --filter @receptionalx/relay build",
    "lint": "pnpm -r lint",
    "db:push": "pnpm --filter @receptionalx/supabase db:push",
    "db:types": "pnpm --filter @receptionalx/supabase db:types",
    "db:reset": "pnpm --filter @receptionalx/supabase db:reset",
    "test": "pnpm -r test",
    "clean": "pnpm -r exec rm -rf node_modules dist .next .turbo",
    "deploy:relay": "fly deploy --config fly.toml --dockerfile Dockerfile.relay"
  },
  "devDependencies": {
    "docx": "^9.5.1",
    "typescript": "^5.3.3"
  },
  "pnpm": {
    "overrides": {
      "react": "18.3.1",
      "react-dom": "18.3.1",
      "@types/react": "^18.2.0"
    }
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=20.0.0"
  }
}
