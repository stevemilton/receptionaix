{
  "expo": {
    "name": "ReceptionAI",
    "slug": "receptionai",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "receptionai",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#344532"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.receptionai.app",
      "infoPlist": {
        "NSCameraUsageDescription": "ReceptionAI needs camera access for profile photos",
        "NSPhotoLibraryUsageDescription": "ReceptionAI needs photo library access for profile photos",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.receptionai.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#344532"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "NOTIFICATIONS"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#344532"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "480f5c1a-a0ba-4bd2-b98b-9dd8926a90f3"
      }
    },
    "owner": "stevemilton"
  }
}
